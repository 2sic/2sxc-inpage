{
  "compileOnSave": true,
  "compilerOptions": {
    //"outFile": "../../dist-test/inpage/inpage.min.js",
    //"outDir": "generated",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "removeComments": false,
    "sourceMap": true,
    "inlineSources": true,
    "target": "es5",
    "allowJs": true,
    "newLine": "CRLF",
    "module": "commonjs",
    "moduleResolution": "node",
    //"typeRoots": [ "./node_modules/@types" ]
    "types": [ "node", "i18next"]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "bin",
    "obj"
  ]
}