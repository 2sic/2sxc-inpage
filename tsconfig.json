{
  "compileOnSave": true,
  "compilerOptions": {
    //"outFile": "../../dist-test/inpage/inpage.min.js",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": true,
    "inlineSources": true,
    "target": "es5",
    "allowJs": true,
    "newLine": "CRLF" 
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "bin",
    "obj"
  ]
}